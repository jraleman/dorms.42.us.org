{
  "name": "nostromo-dorms",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://jraleman.com/nostromo-dorms",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-i18n": "^1.0.1",
    "react-scripts": "1.1.4",
    "semistandard": "^12.0.1",
    "snazzy": "^7.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "node ./node_modules/semistandard/bin/cmd.js --verbose | node ./node_modules/snazzy/bin/cmd.js || true",
    "lint-log": "npm run lint > lint.log ; open lint.log || true",
    "lint-linux-log": "npm run lint > lint.log ; open lint.log || true",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0"
  },
  "semistandard": {
    "ignore": [
      "public/",
      "node_modules"
    ]
  }
}
